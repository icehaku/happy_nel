<%= link_to "Create User", users_path %> <br>
<%= no_pending_research? %> <br>


<h3> Researches: </h3>
<% @researches.reverse.each do |research| %>
  <span> <%= research.created_at %>, <%= research.concluded %> </span>
  <% unless research.concluded %>
    <%= link_to "Force Research Conclusion", force_conclusion_research_path(research) %>
  <% end %> <br>
<% end %>
